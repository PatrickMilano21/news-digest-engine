{
  "mcpServers": {
    "verifier": {
      "type": "stdio",
      "command": "python",
      "args": [
        "mcp-servers/verifier/server.py"
      ],
      "env": {}
    },
    "playwright": {
      "type": "stdio",
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "@playwright/mcp@latest"
      ],
      "env": {}
    },
    "browserbase": {
      "type": "stdio",
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "@browserbasehq/mcp-server-browserbase@latest"
      ],
      "env": {
        "BROWSERBASE_API_KEY": "${BROWSERBASE_API_KEY}",
        "BROWSERBASE_PROJECT_ID": "${BROWSERBASE_PROJECT_ID}"
      }
    }
  }
}